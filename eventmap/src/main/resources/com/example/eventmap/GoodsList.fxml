<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<?import javafx.scene.text.Text?>
<HBox fx:id="goodsBox" spacing="2.5" alignment="CENTER_LEFT" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.eventmap.GoodsListController">
    <CheckBox fx:id="productCheck" mnemonicParsing="false" selected="true"/>
    <TextArea fx:id="productName" prefHeight="42.0" prefWidth="250.0" promptText="商品名" wrapText="true"/>
    <TextArea fx:id="productPrice" prefHeight="42.0" prefWidth="100.0" promptText="価格" wrapText="true"/>
    <Text text="円"/>
    <Button fx:id="deleteProduct" mnemonicParsing="false" text="削除"/>
</HBox>